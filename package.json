{
  "name": "ndotoni-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "./scripts/analyze-bundle.sh",
    "codegen": "graphql-codegen --config codegen.yml",
    "amplify:statements": "pnpm amplify codegen statements",
    "amplify:types": "pnpm amplify codegen types",
    "amplify:codegen": "pnpm run amplify:statements && pnpm run amplify:types",
    "schema:download": "aws appsync get-introspection-schema --api-id tpxpbec6e5crxhu277uknqxoqi --region us-west-2 --format SDL schema.graphql",
    "schema:clean": "node scripts/clean-schema.js",
    "schema:generate": "pnpm run schema:clean && pnpm run amplify:codegen",
    "schema:update": "pnpm run schema:download && pnpm run schema:generate"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@heroicons/react": "^2.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "aws-amplify": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.562.0",
    "nest-ql-schema": "latest",
    "next": "^14.2.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.34.5",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.0.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^14.2.4",
    "@aws-amplify/cli-core": "^2.2.3",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.35",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "last 2 versions",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
